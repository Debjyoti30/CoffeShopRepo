@model List<core.Table.Entry.Coffee>

@{
    ViewBag.Title = "Coffee list";
    Layout = "~/Views/Admin/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<table>
    <tr>
        <th>Title</th>
        <th>Price</th>
    </tr>
    @foreach (var good in Model)
    {
        <tr>
            <td>@good.Title</td>
            <td>@good.Price</td>
            <td>@Html.ActionLink("edit", "CoffeeEntry", new { goodId = good.RowKey })</td>
            <td>@Html.ActionLink("delete", "CoffeeDelete", new { id = good.RowKey })</td>
        </tr>
    }
</table>

@Html.ActionLink("new", "CoffeeEntry")