@model List<AzureCoffeShop.Models.CoffeeModel>

<table>
    <tr>
        <th>Image</th>
        <th>Title</th>
        <th>Price</th>
    </tr>
    @foreach (var good in Model)
    {
        <tr>
            <th><img src="@good.Image" /></th>
            <th>@good.Title</th>
            <th>@good.Price</th>
        </tr>
    }
</table>